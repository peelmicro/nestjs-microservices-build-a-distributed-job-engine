{
  "name": "@jobber/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "generate-ts-proto": "npx protoc --plugin=./node_modules/.bin/ptoroc-gen_ts_proto --ts_proto_out=./types ./proto/*.proto --ts_proto_opt=nestJs=true",
    "serve:all": "nx run-many -t serve -p auth jobs",
    "build:all": "nx run-many -t build -p auth jobs"
  },
  "nx": {
    "targets": {
      "generate-ts-proto": {
        "inputs": [
          "{workspaceRoot}/proto/*.proto"
        ],
        "cacheable": true
      }
    }
  },
  "private": true,
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@golevelup/nestjs-discovery": "^4.0.3",
    "@grpc/grpc-js": "^1.12.6",
